<body class="form-v7">
	<div class="page-content">
		<div class="form-v7-content">
			<div class="form-left">
				<div class="thumbnail-wrapper" style="display: flex; background-color: black;">
					<img src="/images/signup.jpg" style="height: 675px;" class="text-center" alt="세모금 메인" usemap="#signup-map">
					<map name="signup-map">
						<area shape="rect" coords="110,540,225,620" alt="세모금 메인 바로가기" href="/mogeum/1" style="outline: 0;" />
					</map>
				</div>
				<!-- <p class="text-1">회원가입</p> -->
				<!--<p class="text-2">Privaci policy & Term of service</p>-->
			</div>
			<form class="form-detail" action="/users/signUp" method="POST" id="myform">
				<div class="form-row">
					<label for="username">아이디</label>
					<input type="text" name="userId" id="username" class="input-text" required>
				</div>
				<div class="form-row">
					<label for="userName">이름</label>
					<input type="text" name="userName" id="username" class="input-text" required>
				</div>
				<div class="form-row">
					<label for="your_email">이메일</label>
					<input type="email" name="userEmail" id="username" class="input-text" required>
				</div>
				<div class="form-row">
					<label for="your_phone">휴대폰 번호</label>
					<input type="text" name="userPhone" id="userPhone" class="input-text" required>
				</div>
				<div class="form-row">
					<label for="password">비밀번호</label>
					<input type="password" name="userPwd" id="password" class="input-text" required>
				</div>
				<div class="form-row">
					<label for="comfirm_password">비밀번호 확인</label>
					<input type="password" name="userPwdConfirm" id="comfirm_password" class="input-text" required>
				</div>
				<div class="form-row-last">
					<input type="submit" name="register" class="register" value="회원가입">
					<p>또는<a href="/users/login">로그인</a></p>
				</div>
			</form>
		</div>
	</div>
</body>